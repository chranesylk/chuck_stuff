public class BPM
{
    
   public float GetQuater(float bpm)
   {
        return 60.0/bpm;
        
   }
    
   public float GetEighth(float bpm)
   {
        return 60.0 * 0.5/bpm;
        
   }
   
   public float GetSixteenth(float bpm)
   {
        return 60.0 * 0.25/bpm;
        
   }
     
     
     
     }